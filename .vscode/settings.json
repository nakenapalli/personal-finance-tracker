{
    "typescript.tsdk": "node_modules/typescript/lib",
    "css.customData": [
        ".vscode/css-custom-data.json"
    ],
    "tailwindCSS.experimental.classRegex": [
        [
            "cva\\(([^)]*)\\)",
            "[\"'`]([^\"'`]*).*?[\"'`]"
        ],
        [
            "cx\\(([^)]*)\\)",
            "(?:'|\"|`)([^']*)(?:'|\"|`)"
        ]
    ],
    "files.associations": {
        "*.css": "tailwindcss"
    }
}